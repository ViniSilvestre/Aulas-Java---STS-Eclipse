package br.com.generation.aula01;

public class ExemploVariavel02 {

	public static void main(String[] args) {
		
		int var1;
		double var2; 
				
		var1=10;
		var2=10.0;
		
		System.out.println("Valor da 1° Variavel: "+var1);
		System.out.println("Valor da 2° Variavel: "+var2);
		
		var1=var1/4;
		var2=var2/4;
		
		System.out.println("Valor da 1° Variavel após a divisão: "+var1);
		System.out.println("Valor da 2° Variavel após a divisão: "+var2);
		
		
	}
}
